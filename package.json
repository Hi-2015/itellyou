            "prettier --write"
        ],
        "*.ts?(x)": [
            "prettier --parser=typescript --write"
        ]
    },
    "dependencies": {
        "@ant-design/icons": "^4.0.3",
        "@itellyou/itellyou-editor": "^2.2.1",
        "@umijs/preset-react": "1.x",
        "@umijs/test": "^3.0.13",
        "antd": "^4.1.1",
        "cropperjs": "^1.5.6",
        "lint-staged": "^10.0.7",
        "make-error": "^1.3.5",
        "node-htmldiff": "^0.9.3",
        "nprogress": "^0.2.0",
        "nzh": "^1.0.4",
        "ot-json0": "^1.1.0",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-container-query": "^0.11.0",
        "react-document-title": "^2.0.3",
        "react-dom": "^16.12.0",
        "react-infinite-scroller": "^1.2.4",
        "umi": "^3.0.13",
        "underscore.string": "^3.3.5",
        "yorkie": "^2.0.0"
    }
