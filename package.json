{
    "private": true,
    "scripts": {
        "start": "umi dev",
        "build": "umi build",
        "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
